{
    "rules": {
        "curly": [2, "multi-line"],
        "eol-last": [2],
        "indent": [2, 4],
        "quotes": [2, "single"],
        "semi": [2, "always"],
        "strict": [2, "never"],
        "no-console": "off"
    },
    "env": {
        "node": true,
        "es6": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "extends": "eslint:recommended"
}
